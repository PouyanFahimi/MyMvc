
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>SendViewBag</title>
</head>
<body>
    <div> 
        <h1>@ViewBag.Name</h1>
        
        <ul>
            @foreach (var item in ViewBag.listName)
            {
                <li>@item</li>
            }
        </ul>
    </div>
</body>
</html>
